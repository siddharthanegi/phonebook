<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>TestPhonebook</display-name>
  <welcome-file-list>
    <welcome-file>phonebook.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
<servlet>
		<servlet-name>Phone Book</servlet-name>
		<servlet-class>net.example.web.phonebook</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Phone Book</servlet-name>
		<url-pattern>/DataEntry.do</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Phone Query</servlet-name>
		<servlet-class>net.example.web.phonequery</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Phone Query</servlet-name>
		<url-pattern>/ServeQuery.do</url-pattern>
	</servlet-mapping>
	
</web-app>